{
    "name": "devsite/claude-skill-laravel-migration-searcher",
    "description": "Intelligent Laravel migration indexer with Claude AI integration. Automatically analyzes and indexes all migrations for instant search and debugging.",
    "keywords": [
        "laravel",
        "migrations",
        "indexer",
        "search",
        "claude",
        "ai",
        "developer-tools"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Kuriata",
            "email": "adriandj83@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0|^12.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "DevSite\\LaravelMigrationSearcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevSite\\LaravelMigrationSearcher\\MigrationSearcherServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
